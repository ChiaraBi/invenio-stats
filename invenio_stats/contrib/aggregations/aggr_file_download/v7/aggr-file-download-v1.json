{
  "template": "stats-file-download-*",
  "settings": {
    "index": {
      "refresh_interval": "1m"
    }
  },
  "mappings": {
    "dynamic_templates": [
      {
        "date_fields": {
          "match_mapping_type": "date",
          "mapping": {
            "type": "date",
            "format": "date_optional_time"
          }
        }
      }
    ],
    "_all": {
      "enabled": false
    },
    "date_detection": false,
    "dynamic": false,
    "numeric_detection": false,
    "properties": {
      "timestamp": {
        "type": "date",
        "format": "date_optional_time"
      },
      "count": {
        "type": "integer",
        "index": "not_analyzed"
      },
      "file_id": {
        "type": "keyword"
      },
      "file_key": {
        "type": "keyword"
      },
      "bucket_id": {
        "type": "keyword"
      },
      "collection": {
        "type": "keyword"
      }
    }
  },
  "aliases": {
    "stats-file-download": {}
  }
}
