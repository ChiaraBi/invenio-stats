{
  "template": "events-stats-record-view-*",
  "settings": {
\    "index": {
      "refresh_interval": "5s"
    }
  },
  "mappings": {
    "_all": {
      "enabled": false
    },
    "date_detection": false,
    "dynamic": false,
    "numeric_detection": false,
    "properties": {
      "timestamp": {
        "type": "date",
        "format": "strict_date_hour_minute_second"
      },
      "record_id": {
        "type": "keyword"
      },
      "pid_type": {
        "type": "keyword"
      },
      "pid_value": {
        "type": "keyword"
      },
      "labels": {
        "type": "keyword"
      },
      "country": {
        "type": "keyword"
      },
      "visitor_id": {
        "type": "keyword"
      }
    }
  },
  "aliases": {
    "events-stats-record-view": {}
  }
}
